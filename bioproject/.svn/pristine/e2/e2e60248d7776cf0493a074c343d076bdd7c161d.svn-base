	<queryString>
		<![CDATA[SELECT
   conge.`idcon` AS conge_idcon,
     conge.`debut` AS conge_debut,
     conge.`fin` AS conge_fin,
     conge.`type` AS conge_type,
     conge.`idper` AS conge_idper,
     conge.`idmot` AS conge_idmot,
     motif.`idmot` AS motif_idmot,
     motif.`designation` AS motif_designation,
     personnel.`matricule` AS personnel_matricule,
     personnel.`nom` AS personnel_nom,
     personnel.`prenom` AS personnel_prenom,
     affiliation.`idaff` AS affiliation_idaff,
     affiliation.`designation` AS affiliation_designation,
     affiliation.`description` AS affiliation_description,
     affiliation.`type` AS affiliation_type,
     affiliation.`idpar` AS affiliation_idpar,
     affiliation.`codeteam` AS affiliation_codeteam,
     historique.`idhis` AS historique_idhis,
     historique.`Du` AS historique_Du,
     historique.`Au` AS historique_Au,
     historique.`idaff` AS historique_idaff,
     historique.`idper` AS historique_idper,
     historique.`idcat` AS historique_idcat,
     historique_cat.`idhistorique_cat` AS historique_cat_idhistorique_cat,
     historique_cat.`du` AS historique_cat_du,
     historique_cat.`au` AS historique_cat_au,
     historique_cat.`idseq` AS historique_cat_idseq,
     historique_cat.`cle_jour` AS historique_cat_cle_jour,
     historique_cat.`idcat` AS historique_cat_idcat,
     historique_cat.`idper` AS historique_cat_idper,
     categorie.`idcat` AS categorie_idcat,
     categorie.`desigCat` AS categorie_desigCat,
 sequence.`longueurSequence` AS sequence_longueurSequence,
     sequence.`typeSeq` AS sequence_typeSeq,
     sequence.`DesigSeq` AS sequence_DesigSeq,
     sequence.`idseq` AS sequence_idseq,
$P!{paramGroup1} as paramGroup1,
$P!{paramGroup2} as paramGroup2,
$P!{paramGroup3} as paramGroup3,
$P!{paramGroup4} as paramGroup4
FROM
     `conge` conge INNER JOIN `personnel` personnel ON personnel.`idper` = conge.`idper`
     LEFT OUTER JOIN `motif` motif ON conge.`idmot` = motif.`idmot`
     LEFT OUTER JOIN `historique` historique ON personnel.`idper` = historique.`idper`
     LEFT OUTER JOIN `historique_cat` historique_cat ON personnel.`idper` = historique_cat.`idper`
     LEFT OUTER JOIN `categorie` categorie ON historique_cat.`idcat` = categorie.`idcat`
     LEFT OUTER JOIN `affiliation` affiliation ON historique.`idaff` = affiliation.`idaff`
 LEFT OUTER JOIN `sequence` sequence ON historique_cat.`idseq` = sequence.`idseq`
where
$P!{DynamicQuery} and conge.`type`= 'Congé' and historique.`Du` <= conge.`debut` and conge.`debut` <= historique.`Au`]]>
	</queryString>
